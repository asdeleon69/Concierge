@model IEnumerable<ConciergeV1.Models.Tran_Tipo_Transporte>

@{
    ViewBag.Title = "Index";
}

<h2>Tipo Transporte</h2>

<p>
    @Html.ActionLink("Agregar nuevo registro", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Descripción")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Destip)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Codtip }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.Codtip }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.Codtip })
        </td>
    </tr>
}

</table>
